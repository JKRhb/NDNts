{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-shadowed-variable": false,
    "object-literal-sort-keys": [true, "match-declaration-order"],
    "unified-signatures": false,
    "variable-name": [true, "ban-keywords", "check-format", "allow-trailing-underscore"]
  },
  "rulesDirectory": []
}
